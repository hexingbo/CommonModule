    <!-- 自定义进度条-->
    <declare-styleable name="ArcProgress">
        <attr name="borderWidth" format="dimension" />
        <attr name="unprogresColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="tickWidth" format="dimension" />
        <attr name="tickDensity" format="integer" />
        <attr name="bgShow" format="boolean" />
        <attr name="radius" format="dimension" />
        <attr name="arcbgColor" format="color" />
        <attr name="degree" format="integer" />
        <attr name="arcCapRound" format="boolean" />
        <attr name="progressStyle" format="enum">
            <enum name="tick" value="1" />
            <enum name="arc" value="0" />
        </attr>
    </declare-styleable>
    <!--半透明-->
    <style name="bottom_select_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsFloating">true</item><!-- 是否漂现在activity上 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <!--全透明-->
    <style name="trans_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsFloating">true</item><!-- 是否漂现在activity上 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="loginDialogAnim">
        <item name="@android:windowEnterAnimation">@anim/slide_bottom_entry</item>
        <item name="@android:windowExitAnimation">@anim/slide_top_exit</item>
    </style>

